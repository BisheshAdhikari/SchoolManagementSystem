@{
}
@model List<Student> // Assuming you have a list of Student objects to display

@{
    ViewBag.Title = "Student List";
}

<div class="container">
    <h2>Student List</h2>
    <table class="table table-striped">
        <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Date of Birth</th>
                <th>Gender</th>
                <th>Grade</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var student in Model)
            {
                <tr>
                    <td>@student.FirstName</td>
                    <td>@student.LastName</td>
                    <td>@student.DateOfBirth</td>
                    <td>@student.Gender</td>
                    <td>@student.GradeName</td>
                </tr>
            }
        </tbody>
    </table>
</div>

